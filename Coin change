def calculate_change(denominations, quantities, purchase_amount, customer_choice):
    total_change = sum(denom * quant for denom, quant in zip(denominations, quantities))
    change = total_change - purchase_amount
    
    change_denominations = []
    for denom in reversed(denominations):
        num_notes = min(change // denom, quantities[denominations.index(denom)])
        change -= num_notes * denom
        quantities[denominations.index(denom)] -= num_notes
        change_denominations.append((denom, num_notes))
    
    return change_denominations

# Available denominations and their quantities
denominations = [50, 1]
quantities = {50: 6, 1: 5}

# Amount of purchase
purchase_amount = 20 * 100 + 50  # Convert to paise

# Customer choice: 'more' or 'less'
customer_choice = 'more'

change_denominations = calculate_change(denominations, quantities, purchase_amount, customer_choice)

# Output the change denominations
print("Change denominations:")
for denom, num_notes in change_denominations:
    print(f"{num_notes} {'coin' if denom == 1 else 'note'} of {denom} {'paise' if denom == 1 else 'rupees'}")
